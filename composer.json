{
    "name": "yagami271/dtotocsv",
    "description": "Generate Csv file from DTO",
    "keywords": [
        "yagami271",
        "dtotocsv"
    ],
    "homepage": "https://github.com/yagami271/dtotocsv",
    "license": "MIT",
    "authors": [
        {
            "name": "Ismaile ABDALLAH",
            "email": "abdallahismail271@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Yagami\\DtoToCsv\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yagami\\DtoToCsv\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
            "test-coverage": "vendor/bin/phpunit --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
